{
    "plugins": [
        "eslint-plugin-prettier",
        "eslint-plugin-no-cyrillic-string"
    ],
    "rules": {
        "prettier/prettier": "error",
        "no-cyrillic-string/no-cyrillic-string": "error"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module"
    }
}
